cat MinoT_APK_Sentive.sh 
#!/usr/bin/bash

apktool d $1 | grep -EHirn "Authorization|administrators|developers|users|accesskey|admin|aes|api_key|apikey|checkClientTrusted|crypt|http:|https:|password|pinning|secret|SHA256|SharedPreferences|superuser|token|X509TrustManager"

apktool d l.apk -o l;grep -Phro "(https?://)[\w\.-/]+[\"'\`]" uberApk/ | sed 's#"##g' | anew | grep -v "w3\|android\|github\|http://schemas.android\|google\|http://goo.gl"


